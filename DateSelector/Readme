Usage
----
lazy var datePickerLauncher: DatePickerLauncher = {
        let launcher = DatePickerLauncher()
        launcher.delegate = self
        return launcher
    }()
    
datePickerLauncher.showDatePicker()
    
    

// Support 
extension ViewController : RadioButtonControllerDelegate {
    
    // Selected Radio button
    func didSelectRadioButton(selectedRadio: RadioButton) {
    }
    
}

